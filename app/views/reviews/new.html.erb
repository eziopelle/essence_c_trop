<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>
<div class="review_formulaire">
  <%= simple_form_for [@car, @review] do |f| %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Confirm", class: "btn btn-primary" %>
  <% end %>
</div>
